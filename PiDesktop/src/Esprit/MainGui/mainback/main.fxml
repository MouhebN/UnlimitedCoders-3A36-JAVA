<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="myStackPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Esprit.MainGui.mainback.dashboard">
    <children>
        <BorderPane prefHeight="857.0" prefWidth="1200.0" styleClass="window-background" stylesheets="@/Esprit/resources/styles.css">
            <left>
                <VBox alignment="TOP_CENTER" prefHeight="847.0" prefWidth="250.0" spacing="10.0" styleClass="nav-background" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </BorderPane.margin>
                    <children>
                        <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@/Esprit/resources/icons/ic_profile_image.png" />
                            </image>
                            <VBox.margin>
                                <Insets top="20.0" />
                            </VBox.margin>
                        </ImageView>
                        <Label fx:id="receptionNameTv" styleClass="text-sub-heading" text="Admin" />
                        <Separator prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" prefHeight="32.0" prefWidth="250.0">
                            <children>
                                <Label alignment="TOP_LEFT" styleClass="text-sub-heading-light" text="GENERAL">
                                    <HBox.margin>
                                        <Insets left="30.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                        <JFXButton fx:id="navAppointmentsBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0"  onAction="#onLogOutClick" styleClass="nav-button-selected"   text="Liste Des Articles">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>
                        <JFXButton fx:id="navTestReportsBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0" onAction="#onLogOutClick1" styleClass="nav-button-unselected" text="Ajouter Article">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>

                        <Separator prefWidth="200.0" />
                        <HBox alignment="CENTER_LEFT" prefHeight="32.0" prefWidth="250.0">
                            <children>
                                <Label alignment="TOP_LEFT" styleClass="text-sub-heading-light" text=" Other ">
                                    <HBox.margin>
                                        <Insets left="30.0" />
                                    </HBox.margin>
                                </Label>
                            </children>
                        </HBox>
                        <JFXButton fx:id="navSettingsBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0" styleClass="nav-button-unselected" text="SETTINGS">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>
                        <JFXButton fx:id="navReportBug" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0" styleClass="nav-button-unselected" text="REPORT A BUG">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>
                        <JFXButton fx:id="navSupportBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0" styleClass="nav-button-unselected" text="SUPPORT">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>
                        <JFXButton fx:id="navLogOutBtn" alignment="BASELINE_LEFT" graphicTextGap="20.0"  prefHeight="47.0" prefWidth="240.0" styleClass="nav-button-unselected" text="LOG OUT">
                            <padding>
                                <Insets left="30.0" />
                            </padding>
                        </JFXButton>
                    </children>
                </VBox>
            </left>
            <center>
                <AnchorPane fx:id="frameLayout" styleClass="content-background">
                    <BorderPane.margin>
                        <Insets bottom="5.0" right="5.0" top="5.0" />
                    </BorderPane.margin>
                    <padding>
                        <Insets bottom="15.0" left="20.0" right="20.0" top="15.0" />
                    </padding>
                </AnchorPane>
            </center>
        </BorderPane>
    </children>
</StackPane>
